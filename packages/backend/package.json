{
  "name": "@afw/backend",
  "version": "0.0.1",
  "type": "module",
  "description": "ActionFlows Backend - Express API with WebSocket",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "harmony:enforce": "npx tsx src/cli/harmony-enforce.ts"
  },
  "dependencies": {
    "@afw/shared": "workspace:*",
    "chokidar": "^3.5.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "ioredis": "^5.3.0",
    "ws": "^8.14.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.0",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^4.0.18",
    "node-fetch": "^3.3.2",
    "supertest": "^6.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0"
  }
